{% extends 'ZentriumCoreBundle::layoutViewer.html.twig' %}

{% block title %}{{ 'zentrium.viewer.user_profile.title'|trans }}{% endblock %}

{% block content %}
<div class="box box-primary">
  <div class="box-body">
    <dl>
      <dt>{{ 'zentrium.user.field.name'|trans }}</dt>
      <dd>{{ user.name }}</dd>
      <dt>{{ 'zentrium.user.field.firstname'|trans }}</dt>
      <dd>{{ user.firstname }}</dd>
    </dl>
  </div>
</div>
{% endblock %}
